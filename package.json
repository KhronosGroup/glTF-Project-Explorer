  "private": true,
  "homepage": "https://github.khronos.org/glTF-Project-Explorer",
  "dependencies": {
    "@types/dompurify": "^2.3.3",
    "@types/flexsearch": "^0.7.2",
    "@types/jest": "^27.4.0",
    "@types/marked": "^4.0.1",
    "@types/node": "^17.0.10",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "@types/react-redux": "^7.1.22",
    "dompurify": "^2.3.4",
    "flexsearch": "^0.7.21",
    "gh-pages": "^3.2.3",
    "marked": "^4.0.10",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-github-fork-ribbon": "^0.6.0",
    "react-redux": "^7.2.6",
    "redux": "^4.1.2",
    "redux-devtools-extension": "^2.13.9",
    "redux-saga": "^1.1.3",
    "reselect": "^4.1.5",
    "typescript": "4.5.4"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.2",
    "postcss": "^8.4.6",
    "prettier": "^2.5.1",
    "prettier-plugin-tailwindcss": "^0.1.7",
    "tailwindcss": "^3.0.18",
    "react-scripts": "^5.0.0"
  },
  "resolutions": {},
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "predeploy": "yarn build",
    "deploy": "gh-pages -d build -o upstream"
  },
  "eslintConfig": {
    "extends": "react-app",
    "rules": {
      "quotes": [
        "error",
        "double"
      ],
      "comma-dangle": [
        "error",
        {
          "arrays": "always-multiline",
          "exports": "always-multiline",
          "functions": "never",
          "imports": "always-multiline",
          "objects": "always-multiline"
        }
      ]
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
