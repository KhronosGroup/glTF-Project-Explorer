{
  "name": "project-explorer",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://github.khronos.org/glTF-Project-Explorer",
  "dependencies": {
    "@types/dompurify": "^2.2.1",
    "@types/jest": "24.0.18",
    "@types/marked": "^2.0.2",
    "@types/node": "14.14.41",
    "@types/react": "16.14.5",
    "@types/react-dom": "16.9.12",
    "@types/react-redux": "^6.0.14",
    "dompurify": "^2.2.7",
    "gh-pages": "^2.1.1",
    "marked": "^4.0.10",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-github-fork-ribbon": "^0.6.0",
    "react-redux": "^7.2.3",
    "react-scripts": "^4.0.3",
    "redux": "^4.0.5",
    "redux-devtools-extension": "^2.13.9",
    "redux-saga": "^1.1.3",
    "reselect": "^4.0.0",
    "typescript": "4.2.4"
  },
  "resolutions": {
    "react-scripts/webpack-dev-server/**/http-proxy": "1.18.1",
    "react-scripts/webpack/**/elliptic": "6.5.4",
    "react-scripts/webpack/terser-webpack-plugin": "4.2.3",
    "**/lodash": "4.17.21",
    "**/y18n": "4.0.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "predeploy": "yarn build",
    "deploy": "gh-pages -d build -o upstream"
  },
  "eslintConfig": {
    "extends": "react-app",
    "rules": {
      "quotes": [
        "error",
        "double"
      ],
      "comma-dangle": [
        "error",
        {
          "arrays": "always-multiline",
          "exports": "always-multiline",
          "functions": "never",
          "imports": "always-multiline",
          "objects": "always-multiline"
        }
      ]
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
